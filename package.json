{
  "name": "mm-gui-ng2",
  "version": "1.0.0",
  "description": "Angular 2 GUI for Money Manager",
  "main": "index.js",
  "scripts": {
    "_checkdeployenv": "node deploy/nodejs/validate-deploy-env.js",
    "_applyperm": "chmod a+x -R ./deploy/scripts/aws/*.sh",
    "_prepcfstack": "node deploy/nodejs/yaml-merger",
    "_createorupdatestack": "./deploy/scripts/aws/create_or_update_stack.sh",
    "_uploadartifacts": "./deploy/scripts/aws/upload_assets_to_s3.sh",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "prepdeploy": "npm run _checkdeployenv && npm run _applyperm && npm run _createorupdatestack",
    "deploy": "npm run _uploadartifacts",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v8-suresh/mm-gui-ng2.git"
  },
  "keywords": [
    "ng2",
    "angular2",
    "serverless",
    "auth0",
    "learn",
    "bootstrap",
    "webpack"
  ],
  "author": "suresh.sivanantham@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/v8-suresh/mm-gui-ng2/issues"
  },
  "homepage": "https://github.com/v8-suresh/mm-gui-ng2#readme",
  "dependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/core": "~2.1.0",
    "@angular/forms": "~2.1.0",
    "@angular/http": "~2.1.0",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "@angular/router": "~3.1.0",
    "auth0-lock": "^10.5.1",
    "core-js": "^2.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "^2.4.1",
    "json-refs": "^2.1.6",
    "karma": "^1.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "null-loader": "^0.1.1",
    "path": "^0.12.7",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "typescript": "^2.0.2",
    "typings": "^1.3.2",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0",
    "yaml-js": "^0.1.4"
  }
}
